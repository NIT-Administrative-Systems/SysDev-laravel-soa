{
    "name": "northwestern-sysdev/laravel-soa",
    "description": "Laravel bindings for popular Northwestern SOA services.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "northwestern",
        "northwestern university",
        "laravel"
    ],
    "authors": [
        {
            "name": "Nicholas Evans",
            "email": "nick.evans@northwestern.edu"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "northwestern-sysdev/event-hub-php-sdk": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0 || ~4.0"
    },
    "autoload": {
        "psr-4": {
            "Northwestern\\SysDev\\SOA\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Northwestern\\SysDev\\SOA\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "northwestern-sysdev/laravel-websso": "Laravel package for Northwestern University's WebSSO & Duo MFA login."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Northwestern\\SysDev\\SOA\\Providers\\NuSoaServiceProvider"
            ]
        }
    }
}
