{
    "name": "northwestern-sysdev/laravel-soa",
    "description": "Laravel bindings for popular Northwestern SOA services.",
    "type": "library",
    "keywords": [
        "northwestern",
        "northwestern university",
        "laravel"
    ],
    "authors": [
        {
            "name": "Nicholas Evans",
            "email": "nicholas.evans@northwestern.edu"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Northwestern\\SysDev\\SOA\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Northwestern\\SysDev\\SOA\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "northwestern-sysdev/laravel-websso": "Laravel package for Northwestern University's WebSSO & Duo MFA login."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Northwestern\\SysDev\\SOA\\NuSoaServiceProvider"
            ]
        }
    }
}
